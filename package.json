    "@hookform/error-message": "^2.0.1",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.11.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "gh-pages": "^6.3.0",
    "isbot": "^5.1.27",
